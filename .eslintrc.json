{
  "parser": "babel-eslint",
  "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true            
        }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"    
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": 0
  },
  "plugins": [
    
    "react"
  ],
  "settings": {
        "react": {
            "pragma": "React",
            "version": "15.6.1"
        }
    }
}
